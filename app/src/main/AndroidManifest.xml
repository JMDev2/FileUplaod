<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.fileupload" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/myuploads"
        android:label="@string/app_name"
        android:roundIcon="@drawable/myuploads"
        android:supportsRtl="true"
        android:theme="@style/Theme.FileUpload"
        tools:targetApi="31" >
        <activity
            android:name=".ui.ViewImageActivity"
            android:exported="false" />
        <activity
            android:name=".ui.ViewPdfFilesActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity2"
            android:exported="false" />
        <activity android:name=".acounts.UserProfile" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fileupload.ui.MainActivity" />
        </activity>
        <activity
            android:name=".ui.ViewImageFiles"
            android:exported="false" />
        <activity android:name=".ui.UploadFileActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fileupload.ui.MainActivity" />
        </activity>
        <activity
            android:name=".acounts.ForgotPassword"
            android:exported="false" />
        <activity android:name=".ui.UploadImageActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fileupload.ui.MainActivity" />
        </activity>
        <activity
            android:name=".ui.ViewPdfActivity"
            android:exported="false" />
        <activity
            android:name=".acounts.CreateAccountActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="false" />
        <activity
            android:name=".acounts.LoginActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>